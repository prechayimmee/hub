package main

import (
	"fmt"
	"os"
	"os/exec"
)

func main() {
	// Define necessary variables or constants

	// Run the test commands
	runTests()
}

func runTests() {
	// Run the test commands from the Makefile
	cmd := exec.Command("make", "test-all")
	cmd.Stdout = os.Stdout
	cmd.Stderr = os.Stderr

	err := cmd.Run()
	if err != nil {
		fmt.Println("Error running tests:", err)
		os.Exit(1)
	}
}
